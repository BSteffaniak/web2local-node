{
    "name": "@web2local/types",
    "version": "1.0.0",
    "description": "Shared TypeScript types for web2local packages",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run --passWithNoTests",
        "typecheck": "pnpm exec tsc --noEmit"
    },
    "keywords": [
        "web2local",
        "types"
    ],
    "license": "MIT",
    "devDependencies": {
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=20.12.0"
    },
    "files": [
        "dist",
        "src"
    ]
}
