        "state",
        "resume",
        "checkpoint",
        "wal"
    ],
    "license": "MIT",
    "dependencies": {
        "@web2local/types": "0.0.1-alpha.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=20.12.0"
    },
    "files": [
        "dist",
        "src"
    ]
