{
    "name": "@web2local/state",
    "version": "0.0.1-alpha.1",
    "description": "State management with write-ahead logging for web2local resume functionality",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run",
