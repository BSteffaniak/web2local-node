{
    "name": "@web2local/stubs",
    "version": "1.0.0",
    "description": "Stub file generation for web2local",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run --passWithNoTests",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "web2local",
        "stubs",
        "css",
        "typescript"
    ],
    "license": "MIT",
    "dependencies": {
        "@swc/core": "^1.15.3",
        "@web2local/analyzer": "1.0.0",
        "@web2local/ast": "1.0.0",
        "@web2local/http": "1.0.0"
    },
    "devDependencies": {
        "@swc/types": "^0.1.25",
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=20.12.0"
    },
    "files": [
        "dist",
        "src"
    ]
}
