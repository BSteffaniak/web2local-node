{
    "name": "@web2local/sourcemap",
    "version": "0.0.1",
    "description": "Source map parsing, discovery, and extraction for web2local",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "web2local",
        "sourcemap",
        "source-map",
        "extraction",
        "parser"
    ],
    "license": "MIT",
    "dependencies": {
        "@web2local/http": "0.0.1",
        "@web2local/types": "0.0.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=20.12.0"
    },
    "files": [
        "dist",
        "src"
    ]
}
