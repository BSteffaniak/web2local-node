{
    "name": "@web2local/capture",
    "version": "0.0.1",
    "description": "Browser automation and asset capture for web2local",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run --passWithNoTests",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "web2local",
        "capture",
        "browser"
    ],
    "license": "MIT",
    "dependencies": {
        "@web2local/http": "0.0.1",
        "@web2local/manifest": "0.0.1",
        "@web2local/types": "0.0.1",
        "minimatch": "^10.0.1",
        "node-html-parser": "^7.0.1",
        "playwright": "1.54.1"
    },
    "devDependencies": {
        "@types/minimatch": "^5.1.2",
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=20.12.0"
    },
    "files": [
        "dist",
        "src"
    ]
}
