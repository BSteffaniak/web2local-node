{
    "name": "@web2local/cli",
    "version": "1.0.0",
    "description": "CLI entry point for web2local",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "web2local-cli": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run --passWithNoTests",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "web2local",
        "cli"
    ],
    "license": "MIT",
    "dependencies": {
        "@web2local/analyzer": "workspace:*",
        "@web2local/ast": "workspace:*",
        "@web2local/cache": "workspace:*",
        "@web2local/capture": "workspace:*",
        "@web2local/http": "workspace:*",
        "@web2local/manifest": "workspace:*",
        "@web2local/rebuild": "workspace:*",
        "@web2local/scraper": "workspace:*",
        "@web2local/server": "workspace:*",
        "@web2local/stubs": "workspace:*",
        "@web2local/types": "workspace:*",
        "chalk": "^5.3.0",
        "commander": "^14.0.2",
        "ora": "^8.1.0",
        "picocolors": "^1.1.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "dist",
        "src"
    ]
}
