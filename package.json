{
  "name": "top-secret-source-reverse-engineerer-9001",
  "version": "1.0.0",
  "description": "CLI tool to extract and reconstruct original source code from publicly available source maps",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "top-secret-source-reverse-engineerer-9001": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretty": "prettier --check \"**/*.{js,cjs,mjs,jsx,ts,tsx}\"",
    "pretty:write": "prettier --write \"**/*.{js,cjs,mjs,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm typecheck && pnpm lint && pnpm pretty"
  },
  "keywords": [
    "sourcemap",
    "source-map",
    "reverse-engineering",
    "cli"
  ],
  "license": "MIT",
  "devDependencies": {
    "@swc/types": "^0.1.25",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-diff": "^2.0.3",
    "msw": "^2.4.0",
    "prettier": "^3.7.3",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@swc/core": "^1.15.3",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "node-html-parser": "^6.1.12",
    "ora": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
