{
    "name": "web2local",
    "version": "1.0.0",
    "description": "CLI tool to extract and reconstruct original source code from publicly available source maps",
    "type": "module",
    "bin": {
        "web2local": "./packages/web2local/dist/index.mjs"
    },
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "test": "vitest run",
        "test:watch": "vitest run --watch",
        "lint": "turbo lint",
        "lint:fix": "eslint --fix .",
        "pretty": "prettier --check \"**/*.{js,cjs,mjs,jsx,ts,tsx}\"",
        "pretty:write": "prettier --write \"**/*.{js,cjs,mjs,jsx,ts,tsx}\"",
        "typecheck": "turbo typecheck",
        "validate": "pnpm typecheck && pnpm lint && pnpm pretty",
        "clean": "rm -rf packages/*/dist node_modules/.cache",
        "cli": "jiti packages/cli/src/run.ts"
    },
    "keywords": [
        "sourcemap",
        "source-map",
        "reverse-engineering",
        "cli",
        "web2local"
    ],
    "license": "MIT",
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.1",
        "@swc/core": "^1.15.3",
        "@swc/types": "^0.1.25",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-diff": "^2.0.3",
        "globals": "^15.15.0",
        "jiti": "^2.6.1",
        "msw": "^2.4.0",
        "playwright": "^1.54.1",
        "prettier": "^3.7.3",
        "turbo": "^2.5.4",
        "typescript": "^5.3.0",
        "vitest": "^2.1.0"
    },
    "engines": {
        "node": ">=20.12.0"
    },
    "packageManager": "pnpm@10.24.0"
}
